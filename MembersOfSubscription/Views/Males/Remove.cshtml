@model Male

@{
	ViewData["Title"] = "Remove Member";
}


<div class="container col-8">
<h1 class="text-primary">Remove Male Member</h1>
<form method="post" asp-action="Remove">
	<div class="mb-3">
		<label asp-for="Name"></label>
		<input asp-for="Name" class="form-control" readonly/>
	</div>
	<div class="mb-3">
		<label asp-for="Age"></label>
		<input asp-for="Age" class="form-control" readonly/>
	</div>
	<div class="mb-3">
		<label asp-for="Height"></label>
		<input asp-for="Height" class="form-control" readonly/>
	</div>
	<div class="mb-3">
		<label asp-for="FavSport"></label>
		<input asp-for="FavSport" class="form-control" readonly/>
	</div>
	<button type="submit" class="btn btn-danger" style="width:150px">Remove</button>
	<a asp-controller="Males" asp-action="Index" class="btn btn-secondary"
	style="width:150px">Cancel</a>
</form>
</div>